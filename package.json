{
  "name": "mxspotybrokeneyetest",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server/index.js",
  "scripts": {
    "test": "node tests/test-runner-optimized.js",
    "test:basic": "node tests/test-runner-basic.js",
    "test:complete": "node tests/test-runner-complete.js",
    "test:metrics": "node tests/metrics-working.js",
    "test:metrics-only": "node tests/test-runner-complete.js --metrics-only",
    "test:jest": "jest tests/routes-optimized.test.js",
    "test:coverage": "c8 --reporter=lcov --reporter=text npm test",
    "test:unit": "node tests/test-runner-basic.js --unit-only",
    "test:integration": "node tests/test-runner-complete.js --integration",
    "test:e2e": "node tests/test-runner-complete.js --e2e",
    "test:performance": "node tests/test-performance.js",
    "lint": "eslint src/ tests/ --ext .js,.mjs,.json",
    "lint:fix": "eslint src/ tests/ --ext .js,.mjs,.json --fix",
    "format": "prettier --write \"src/**/*.{js,mjs,json}\" \"tests/**/*.{js,mjs,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,mjs,json}\" \"tests/**/*.{js,mjs,json}\"",
    "security": "npm audit && node scripts/security-check.js",
    "start": "node src/server/index.js",
    "serve": "node src/server/index.js",
    "dev": "node src/server/index.js",
    "build": "echo 'Build step - optimizing assets...' && node scripts/build-optimize.js",
    "healthcheck": "curl -f http://localhost:${PORT:-3000}/health || exit 1"
  },
  "author": "",
  "license": "GPL-3.0",
  "description": "",
  "dependencies": {
    "algorith": "^1.0.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "prom-client": "^15.1.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "autocannon": "^7.15.0",
    "c8": "^10.1.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^30.0.5",
    "license-checker": "^25.0.1",
    "open": "^10.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  }
}
